<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.drivenext.app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="34" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_MEDIA_IMAGES" />
    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="32" />

    <permission
        android:name="com.drivenext.app.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.drivenext.app.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:label="@string/app_name"
        android:testOnly="true"
        android:theme="@style/Theme.DriveNext" >
        <activity
            android:name="com.drivenext.app.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.DriveNext" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.drivenext.app.SplashActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.OnboardingActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.GettingStartedActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.LoginActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.SignUp1Activity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.SignUp2Activity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.SignUp3Activity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.CongratulationsActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.NoConnectionActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.SearchResultsActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.CarDetailsActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.BookingActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.BookingSuccessActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.MyBookingsActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.BookingDetailsActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.FavoritesActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.SettingsActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.ProfileActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.BecomeOwnerActivity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.AddCarStep1Activity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.AddCarStep2Activity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.AddCarStep3Activity"
            android:theme="@style/Theme.DriveNext" />
        <activity
            android:name="com.drivenext.app.AddCarSuccessActivity"
            android:theme="@style/Theme.DriveNext" />

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.drivenext.app.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.profileinstaller.ProfileInstallerInitializer"
                android:value="androidx.startup" />
        </provider>

        <receiver
            android:name="androidx.profileinstaller.ProfileInstallReceiver"
            android:directBootAware="false"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.DUMP" >
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.INSTALL_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SKIP_FILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SAVE_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.BENCHMARK_OPERATION" />
            </intent-filter>
        </receiver>
    </application>

</manifest>